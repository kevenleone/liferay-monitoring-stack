{
    "cpu": 1,
    "environments": {
        "infra": {
            "deploy": false
        }
    },
    "id": "__PROJECT_ID__",
    "kind": "Deployment",
    "livenessProbe": {
        "httpGet": {
            "path": "/",
            "port": 3000
        }
    },
    "loadBalancer": {
        "targetPort": 3000
    },
    "memory": 1024,
    "readinessProbe": {
        "httpGet": {
            "path": "/",
            "port": 3000
        }
    },
    "scale": 1
}
